{
  "name": "postgresnode",
  "version": "1.0.0",
  "description": "postgres node api",
  "main": "index.js",
  "scripts": {
    "test": "npm run jest",
    "dev": "nodemon -r dotenv/config index.js",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migration": "npx sequelize-cli migration:generate --name",
    "db:g:seed": "npx sequelize-cli seed:generate --name",
    "db:seeds": "npx sequelize-cli db:seed:all"
  },
  "author": "rthrcrsn",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  }
}
